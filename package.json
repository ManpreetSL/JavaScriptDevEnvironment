		"@babel/core": "^7.11.1",
		"@babel/preset-env": "^7.11.0",
		"chai": "3.5.0",
		"chalk": "1.1.3",
		"cheerio": "0.22.0",
		"compression": "1.6.2",
		"cross-env": "3.1.3",
		"css-loader": "0.25.0",
		"eslint": "3.8.1",
		"eslint-plugin-import": "2.0.1",
		"eslint-watch": "2.1.14",
		"express": "4.14.0",
		"extract-text-webpack-plugin": "1.0.1",
		"html-webpack-plugin": "2.22.0",
		"jsdom": "9.8.0",
		"json-schema-faker": "0.3.6",
		"json-server": "0.8.22",
		"mocha": "3.1.2",
